      "MaxValue": 10000.0
    },
    "SpindleSpeed": {
      "MinValue": 0,
      "MaxValue": 65535
    },
    "ToolNumber": {
      "MinValue": 0,
      "MaxValue": 99
    }
  },
  "ProcessingOptions": {
    "DelayBetweenCommandsMs": 100,
    "VerifyWritesEnabled": false,
    "ContinueOnError": true,
    "MaxRetries": 3
  }
